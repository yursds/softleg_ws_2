    <collision>
    <surface>
      <friction>
        <ode>
          <!-- <mu>5.0</mu> -->
          <!-- <mu1>1.5</mu1> 
          <mu2>1.5</mu2>  -->
          <!-- <fdir1>0 0 1</fdir1> -->
          <!-- <slip1>0.0</slip1> -->
